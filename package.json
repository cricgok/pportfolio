    "react-bootstrap": "^2.10.0",
    "react-bootstrap-icons": "^1.10.3",
    "react-multi-carousel": "^2.8.4",
    "react-on-screen": "^2.1.1",
    "react-router-dom": "^6.21.3",
    "react-router-hash-link": "^2.4.3",
